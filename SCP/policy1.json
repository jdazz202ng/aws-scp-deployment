{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyUnapprovedServices",
            "Effect": "Deny",
            "Action": [
                "*"
            ],
            "Resource": "*",
            "Condition": {
                "StringNotEquals": {
                    "aws:ServiceName": [
                        "ec2",
                        "s3",
                        "dynamodb",
                        "lambda"
                    ]
                }
            }
        }
    ]
}
